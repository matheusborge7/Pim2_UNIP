{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compilar PIM em C",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-Wall",
        "-Wextra",
        "-o",
        "PIM.exe",
        "PIM.C"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}
